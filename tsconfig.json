{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "declaration": false,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "baseUrl": "./",
    "lib": [
      "es2019"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@routes/*": [
        "src/app/routes/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*"
  ]
}
